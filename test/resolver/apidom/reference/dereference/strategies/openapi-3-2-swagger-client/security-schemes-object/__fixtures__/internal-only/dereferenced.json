[
  {
    "openapi": "3.2.0",
    "info": {
      "title": "Test API",
      "version": "1.0.0"
    },
    "paths": {
      "/users": {
        "get": {
          "responses": {
            "200": {
              "description": "Success"
            }
          },
          "security": [
            {
              "ApiKey": []
            }
          ]
        },
        "post": {
          "responses": {
            "201": {
              "description": "Created"
            }
          },
          "security": [
            {
              "OAuth2": ["write"]
            }
          ]
        }
      }
    },
    "components": {
      "securitySchemes": {
        "ApiKey": {
          "type": "apiKey",
          "in": "header",
          "name": "X-API-KEY"
        },
        "OAuth2": {
          "type": "oauth2",
          "flows": {
            "authorizationCode": {
              "authorizationUrl": "https://example.com/oauth/authorize",
              "tokenUrl": "https://example.com/oauth/token",
              "scopes": {
                "read": "Read access",
                "write": "Write access"
              }
            }
          }
        }
      }
    }
  }
]
